{
  "expo": {
    "name": "صلاتي",
    "slug": "salatey",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "salatey",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.salatey.plus",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8749426160957410~3356576250"
      },
      "googleServicesFile":"./GoogleService-Info.plist",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Salaty App - Need the permission to get your current location , To follow prayer times according to the area you are in"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "package": "com.salatey.plus",
      "googleServicesFile": "./google-services.json",
      "permissions": [
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-8749426160957410~3192924727"
      },
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
